//lab 1, task 1
#include <iostream>
using namespace std;
int main() {
  float a, b, c, temp;
  cin >> a;
  cin >> b;
  cin >> c;
  if (a > temp) {
   temp = a;
  }
   if (b > temp) {
   temp = b;
  }
   if (c > temp) {
   temp = c;
  }
  cout << "Максимальное:" << temp << endl;
  temp=a;
   
   if ( temp>b ) {
   temp = b;
  }
   if ( temp>c) {
   temp = c;
  }
 cout << "Минимальное:" << temp << endl; 
}

//lab 1, task 2
#include <iostream>
using namespace std;
int main() {
  int a, b, c;
  cin >> a;
  cin >> b;
  cin >> c;
  if (a > 2 and a < 5) {
    cout << a << " принадлежит промежутку от 2 до 5" << endl;
  }
  if (b > 2 and b < 5) {
    cout << b << " принадлежит промежутку от 2 до 5" << endl;
  } 
  if (c > 2 and c < 5) {
    cout << c << " принадлежит промежутку от 2 до 5" << endl;  
  } 
  if (a == 2 || a == 5) {
    cout << a << " лежит на границе" << endl;
  }
  if (b == 2 || b == 5) {
    cout << b << " лежит на границе" << endl;
  } 
  if (c == 2 || c == 5) {
    cout << c << " лежит на границе" << endl;  
  } 
 if (a < 2 || a > 5) {
    cout << a << " не принадлежит промежутку от 2 до 5" << endl;
  }
  if (b < 2 || b > 5) {
    cout << b << " не принадлежит промежутку от 2 до 5" << endl; 
  } 
  if (c < 2 || c > 5) {
    cout << c << " не принадлежит промежутку от 2 до 5" << endl; 
  } 
  return 0;
}

//lab 1, task 4 WORK IN PROGRESS!!!
#include <iostream>
#include <cmath>
using namespace std;
int main() {
 float x, x1, x2, D, a, b, c;
 cout << "Введите коэффицент а" << endl;
 cin >> a;
 cout << "Введите коэффицент b" << endl;
 cin >> b;
 cout << "Введите коэффицент c" << endl;
 cin >> c;
 if (((a == 0) && (!((a == 0) && (b == 0) && (c == 0))))) {
   x = -(c / b);
   cout << "х равен " << x << endl;
 }
 if ((b == 0) && (!((a == 0) && (b == 0) && (c == 0)))) {
  x1 = sqrt(-(c / a));
  x2 = -sqrt(-(c / a));
  cout << "Корни уравнения " << x1 << ", " << x2 << endl;
 }
 if ((c == 0) && (!((a == 0) && (b == 0) && (c == 0)))) {
  x1 = 0;
  x2 = - (b / a);
  cout << "Корни уравнения " << x1 << ", " << x2 << endl;
 }
 if ((a != 0) && (b != 0) && (c != 0)) {
  D = b*b - 4*a*c;
  if (D > 0) {
   x1 = (-b + sqrt(D)) / 2*a;
   x2 = (-b - sqrt(D)) / 2*a;
   cout << "Корни уравнения " << x1 << ", " << x2 << endl;
  }
  if (D == 0) {
    x = - (b / 2*a);
  }
  if (D < 0) {
    cout << "Решений нет" << endl;
  }
  }
 if ((((a == 0) && (c == 0)) || ((b == 0) && (c == 0))) && (!((a == 0) && (b == 0) && (c == 0)))) {
   x = 0;
   cout << "х равен " << x << endl;
 }
 if (((a == 0) && (b == 0)) && (!((a == 0) && (b == 0) && (c == 0)))) {
  cout << "Решений нет" << endl;
 }
 if ((a == 0) && (b == 0) && (c == 0)) {
   cout << "Бесконечно много решений" << endl;
 }
 return 0;
}
